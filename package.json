{
  "name": "react-falcor-graph",
  "version": "0.1.0",
  "description": "React-Falcor bindings for declarative data fetching",
  "main": "dist/index.js",
  "scripts": {
    "dev:examples": "webpack-dev-server --config webpack.config.examples.js --devtool eval --progress",
    "perf": "node --expose-gc test/perf",
    "test:unit": "node test/unit",
    "test:integration": "jest",
    "lint": "eslint --ext .jsx --ext .js ./src ./test",
    "build": "babel src/ -d dist --source-maps",
    "validate": "npm run test:unit && npm run test:integration && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameslaneconkling/react-falcor-graph.git"
  },
  "keywords": [
    "react",
    "falcor",
    "graph"
  ],
  "author": "jameslaneconkling",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jameslaneconkling/react-falcor-graph/issues"
  },
  "homepage": "https://github.com/jameslaneconkling/react-falcor-graph#readme",
  "dependencies": {
    "recompose": "^0.24.0",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "@graphistry/falcor": "^2.9.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "benchmark": "^2.1.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-plugin-react": "^7.3.0",
    "falcor": "^0.1.17",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "tape": "^4.8.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
